{
  "$schema": "https://json.schemastore.org/package",
  "name": "am-cookies",
  "description": "Simple and versatile GDPR compatible Cookie Compliance Web Component",
  "version": "2.0.7",
  "type": "module",
  "main": "./build/index.js",
  "module": "./build/index.js",
  "types": "./build/index.d.ts",
  "unpkg": "./build/unpkg.js",
  "exports": {
    ".": {
      "import": "./build/index.js",
      "node": "./build/index.js",
      "types": "./build/index.d.ts"
    }
  },
  "repository": {
    "url": "https://github.com/aarsteinmedia/am-gdpr.git",
    "type": "git"
  },
  "bugs": "https://github.com/aarsteinmedia/am-gdpr/issues",
  "dependencies": {
    "js-cookie": "^3.0.5"
  },
  "devDependencies": {
    "@csstools/postcss-cascade-layers": "^5.0.2",
    "@custom-elements-manifest/analyzer": "^0.11.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@swc/core": "^1.15.1",
    "@types/gtag.js": "^0.0.20",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^24.10.0",
    "@types/postcss-flexbugs-fixes": "^5.0.3",
    "autoprefixer": "^10.4.22",
    "esbuild": "^0.27.0",
    "eslint": "^9.39.1",
    "eslint-config-sheriff": "^31.0.0",
    "eslint-plugin-perfectionist": "^4.15.1",
    "globals": "^16.5.0",
    "jiti": "^2.6.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-minify": "^1.2.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.1.0",
    "rollup": "^4.53.2",
    "rollup-plugin-dts": "^6.2.3",
    "rollup-plugin-html-literals": "^2.0.1",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-opener": "^0.1.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-summary": "^3.0.1",
    "rollup-plugin-swc3": "^0.12.1",
    "rollup-plugin-typescript-paths": "^1.5.0",
    "stylelint": "^16.25.0",
    "stylelint-config-recommended": "^17.0.0",
    "tsc-alias": "^1.8.16",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4"
  },
  "scripts": {
    "test": "wtr",
    "prebuild": "rimraf ./build",
    "build": "rollup -c --configPlugin typescript",
    "prebuild:types": "rimraf ./types",
    "build:types": "tsc -p ./tsconfig.build.json && tsc-alias",
    "build:cem": "npx cem analyze --config 'cem.config.js'",
    "prod": "pnpm build:types && pnpm build && pnpm build:cem",
    "dev": "rollup -cw --configPlugin typescript --environment NODE_ENV:development",
    "lint:js": "eslint",
    "lint:js:fix": "eslint --fix",
    "lint:css": "stylelint **/*.css",
    "lint:css:fix": "stylelint **/*.css --fix"
  },
  "license": "GPL-2.0-or-later",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "build",
    "CHANGELOG.md",
    "custom-elements.json",
    "README.md"
  ],
  "browserslist": {
    "production": [
      ">0.3%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "customElements": "custom-elements.json",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@swc/core",
      "esbuild",
      "rs-module-lexer",
      "unrs-resolver"
    ]
  }
}